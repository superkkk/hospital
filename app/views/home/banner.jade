#banner-image.carousel.slide(data-ride='carousel')
  ol.carousel-indicators
    li.active(data-target='#banner-image', data-slide-to='0')
    li(data-target='#banner-image', data-slide-to='1')
    li(data-target='#banner-image', data-slide-to='2')
    li(data-target='#banner-image', data-slide-to='3')
  .carousel-inner(role='listbox')
    .item.active
      img(src='/img/banner2.jpg')
    .item
      img(src='/img/banner3.jpg')
    .item
      img(src='/img/banner4.jpg')
    .item
      img(src='/img/banner5.jpg')
